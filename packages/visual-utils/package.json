{
  "name": "@essex/visual-utils",
  "version": "1.0.0",
  "description": "A collection of utilities to help in the creation of powerbi visuals",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint {src,spec}/**/*.ts",
    "docs": "typedoc --out docs --ignoreCompilerErrors ./ --externalPattern **/node_modules/**/*.* --exclude **/src/**/*.spec.ts",
    "mocha": "mocha --require ts-node/register \"src/**/*.spec.ts\"",
    "test": "npm run clean && tsc && npm-run-all docs lint mocha"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/Essex-PowerBI-visuals-base"
  },
  "keywords": [
    "PowerBI",
    "Base"
  ],
  "author": "David Tittsworth <david.tittsworth@atsid.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "font-awesome": "^4.6.1",
    "lodash": "3.6.0",
    "lodash.assignin": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.0",
    "powerbi-models": "^0.11.3",
    "powerbi-visuals-tools": "^1.7.0"
  },
  "devDependencies": {
  }
}