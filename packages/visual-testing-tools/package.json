  "license": "MIT",
  "dependencies": {
    "font-awesome": "^4.6.1",
    "jquery": "^2.2.3",
    "lodash": "3.6.0",
    "powerbi-visuals-tools": "^1.7.2"
  },
  "devDependencies": {
  }
}
