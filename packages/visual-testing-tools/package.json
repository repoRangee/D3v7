{
  "name": "@essex/visual-testing-tools",
  "version": "1.0.0",
  "description": "A set of testing tools for ease of testing within PowerBI",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint {src,spec}/**/*.ts",
    "docs": "typedoc --out docs --ignoreCompilerErrors ./ --externalPattern **/node_modules/**/*.* --exclude **/src/**/*.spec.ts",
